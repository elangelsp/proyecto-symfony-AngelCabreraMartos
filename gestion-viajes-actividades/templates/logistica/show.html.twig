{% extends 'base.html.twig' %}

{% block title %}Logistica{% endblock %}

{% block body %}
    <h1>Logistica</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ logistica.id }}</td>
            </tr>
            <tr>
                <th>Tipo_viaje</th>
                <td>{{ logistica.tipoViaje }}</td>
            </tr>
            <tr>
                <th>Destino_lugar</th>
                <td>{{ logistica.destinoLugar }}</td>
            </tr>
            <tr>
                <th>Medio_transporte</th>
                <td>{{ logistica.medioTransporte }}</td>
            </tr>
            <tr>
                <th>Salida</th>
                <td>{{ logistica.salida ? logistica.salida|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Llegada</th>
                <td>{{ logistica.llegada ? logistica.llegada|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_logistica_index') }}">back to list</a>

    <a href="{{ path('app_logistica_edit', {'id': logistica.id}) }}">edit</a>

    {{ include('logistica/_delete_form.html.twig') }}
{% endblock %}
