        </tbody>
    </table>

    <a href="{{ path('app_viajero_index') }}">back to list</a>

    <a href="{{ path('app_viajero_edit', {'id': viajero.id}) }}">edit</a>

    {{ include('viajero/_delete_form.html.twig') }}
